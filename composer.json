{
    "name": "tecnodesignc/blog-module",
    "description": "Blog module for EncoreCms",
    "type": "encore-module",
    "keywords": [
        "blog",
        "encoredcms",
        "journal"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "composer/installers": "~1.0",
        "tecnodesignc/core-module": "~1.0",
        "cviebrock/eloquent-sluggable": "8.*",
        "tecnodesignc/tag-module": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "orchestra/testbench": "3.8.*",
    },
    "autoload-dev": {
        "psr-4": {
            "Modules\\Blog\\": ".",
            "Modules\\": "Modules/"
        }
    },
    "autoload": {
        "psr-4": {
            "Modules\\Blog\\": "."
        }
    },
    "authors": [
        {
            "name": "Marcos Ratvia",
            "email": "marcos21.009@gmail.com",
            "role": "Developer"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
