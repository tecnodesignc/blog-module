{
    "name": "tecnodesignc/blog-module",
    "description": "Blog module for EncoreCMS 2.0",
    "type": "encore-module",
    "keywords": [
        "blog",
        "encoredcms",
        "journal"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "composer/installers": "^2.1",
        "tecnodesignc/core-module": "~3.0",
        "cviebrock/eloquent-sluggable": "12.*",
        "tecnodesignc/tag-module": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "orchestra/testbench": "3.8.*"
    },
    "autoload-dev": {
        "psr-4": {
            "Modules\\Blog\\": ".",
            "Modules\\": "modules/"
        }
    },
    "autoload": {
        "psr-4": {
            "Modules\\Blog\\": "."
        }
    },
    "authors": [
        {
            "name": "Marcos Ratvia",
            "email": "marcos21.009@gmail.com",
            "role": "Developer"
        }
    ],
    "post-install-cmd": [

    ],
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
